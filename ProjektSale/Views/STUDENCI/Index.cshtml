@model IEnumerable<ProjektSale.STUDENCI>

@{
    ViewBag.Title = "Studenci";
}

<h2>Lista studentów</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IMIE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NAZWISKO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NR_TELEFONU)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KIERUNKI.NAZWA_KIERUNKU)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IMIE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NAZWISKO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NR_TELEFONU)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KIERUNKI.NAZWA_KIERUNKU)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.NR_ALBUMU }) |
            @Html.ActionLink("Details", "Details", new { id=item.NR_ALBUMU }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.NR_ALBUMU })
        </td>
    </tr>
}

</table>
